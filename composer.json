{
    "name": "keboola/dockerbundle",
    "type": "library",
    "description": "Component for running Docker images in KBC",
    "authors": [
        {
            "name": "Keboola",
            "email": "devel@keboola.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "keboola/php-utils": "^2.3.1",
        "keboola/oauth-v2-php-client": "^2.3.0",
        "keboola/gelf-server": "^2.0.0",
        "keboola/output-mapping": "^9.0.0",
        "keboola/staging-provider": "^2.0",
        "symfony/validator": "^2.8|^4.1",
        "symfony/http-foundation": "^2.8|^4.1",
        "symfony/monolog-bridge": "^2.8|^4.1",
        "vkartaviy/retry": "^0.2",
        "keboola/object-encryptor": "^0.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27",
        "squizlabs/php_codesniffer": "^3.3.2"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Keboola\\DockerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keboola\\DockerBundle\\Tests\\": "Tests/"
        }
    }
}
