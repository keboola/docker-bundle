{
    "name": "keboola/dockerbundle",
    "type": "symfony-bundle",
    "description": "Component for running Docker images in KBC",
    "keywords": [],
    "authors": [
        {
            "name": "Keboola",
            "email": "devel@keboola.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/keboola/syrup"
        }
    ],
    "require": {
        "keboola/syrup": "dev-encryption-lib",
        "keboola/php-utils": "^2.0",
        "symfony/symfony": "^2.8",
        "symfony/console": "^2.8",
        "keboola/oauth-v2-php-client": "^2.2",
        "keboola/gelf-server": "^1.0",
        "keboola/input-mapping": "^3.3",
        "keboola/output-mapping": "^3.0",
        "vkartaviy/retry": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.2",
        "symfony/var-dumper": "^2.8",
        "codeclimate/php-test-reporter": "^0.4",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "build-bootstrap": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Keboola\\DockerBundle": ""
        }
    },
    "target-dir": "Keboola/DockerBundle",
    "extra": {
        "symfony-app-dir": "vendor/keboola/syrup/app",
        "symfony-web-dir": "vendor/keboola/syrup/web",
        "syrup-app-name": "docker"
    }
}
