{
    "name": "keboola/dockerbundle",
    "type": "library",
    "description": "Component for running Docker images in KBC",
    "authors": [
        {
            "name": "Keboola",
            "email": "devel@keboola.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "keboola/php-utils": "^2.3.1",
        "keboola/oauth-v2-php-client": "^2.3.0",
        "keboola/gelf-server": "^2.0",
        "keboola/input-mapping": "^15.0",
        "keboola/output-mapping": "^17.0",
        "keboola/staging-provider": "^4.0",
        "keboola/storage-api-php-client-branch-wrapper": "^2.0",
        "symfony/validator": "^4.4|^5.4",
        "symfony/http-foundation": "^4.4|^5.4",
        "symfony/monolog-bridge": "^4.4|^5.4",
        "vkartaviy/retry": "^0.2",
        "keboola/object-encryptor": "^0.3",
        "mustache/mustache": "^2.13",
        "keboola/common-exceptions": "^1.0",
        "keboola/sandboxes-api-php-client": "^6.7",
        "php": ">=7.4",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.3.2",
        "monolog/monolog": "^2.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Keboola\\DockerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keboola\\DockerBundle\\Tests\\": "Tests/"
        }
    }
}
