{
    "name": "keboola/dockerbundle",
    "type": "library",
    "description": "Component for running Docker images in KBC",
    "authors": [
        {
            "name": "Keboola",
            "email": "devel@keboola.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-zip": "*",
        "keboola/artifacts": "^0.2.0",
        "keboola/common-exceptions": "^1.0",
        "keboola/gelf-server": "^3.0",
        "keboola/input-mapping": "^16.1",
        "keboola/oauth-v2-php-client": "^3.0",
        "keboola/object-encryptor": "^2.0",
        "keboola/output-mapping": "^19.0.0",
        "keboola/php-utils": "^2.3.1",
        "keboola/sandboxes-api-php-client": "^6.7",
        "keboola/staging-provider": "^5.1",
        "keboola/storage-api-client": "^13.4|^14.0",
        "keboola/storage-api-php-client-branch-wrapper": "^3.0",
        "monolog/monolog": "^2.5",
        "mustache/mustache": "^2.13",
        "symfony/http-foundation": "^5.4",
        "symfony/monolog-bridge": "^5.4",
        "symfony/validator": "^5.4",
        "vkartaviy/retry": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.3.2",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-symfony": "^1.2"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Keboola\\DockerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keboola\\DockerBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "lock": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
